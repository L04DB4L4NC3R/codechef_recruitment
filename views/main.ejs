	</section>
	<!-- section: contact -->
	<section id="contact" class="section green" style="background-color: #f3caab">
		<div class="container">
			<h4 style="color: #444">Provide Us with your details</h4>
			
			<div class="row">
				<div class="span12">
					<div class="cform" id="contact-form">
						<form action="/record" method="post" role="form" class="contactForm">
							<div class="row">
								<div class="span2"></div>
								<div class="span4">
									<div class="field form-group">
										<input type="text" name="name" class="form-control" id="name" placeholder="Your Name" required />
										
									</div>
									<div class="field form-group">
										<input type="text" class="form-control" name="regno" id="Registration" placeholder="Registration number" required />
									
									</div>
									<div class="field form-group">
										<input type="text" class="form-control" name="room" id="Block" placeholder="Your Block and Room number" required />
									
									</div>
									
								</div>
								<div class="span4">
									<div class="field form-group">
										<input type="text" class="form-control" name="email" id="email" placeholder=" VIT Email Address" required />
										
									</div>
									<div class="field form-group">
										<input type="text" class="form-control" name="phone" id="phno" placeholder="Your Whatsapp Number" required />
										
									</div>
									<div class="field form-group">
										<select>
											<option>Gender</option>
											<option>Male</option>
											<option>Female</option>
										</select>
										
									</div>
								</div>
							</div>
							<div class="row">
								<div class="span4"></div>
								<div class="span4">
									<input type="submit" value="NEXT" class="btn btn-theme pull-left" style="width: 100%">
								</div>
							</div>
						</form>
					</div>
				</div>
				<!-- ./span12 -->
			</div>
		</div>
	</section>
	
